<image>
	<profiles>
		<profile name="LXQt-Live" description="LXQt Spin Live DVD">
			<requires profile="LXQt-Desktop"/>
			<requires profile="LiveInstall"/>
		</profile>
		<profile name="LXQt-Disk" description="LXQt Spin Install Disk">
			<requires profile="LXQt-Desktop"/>
