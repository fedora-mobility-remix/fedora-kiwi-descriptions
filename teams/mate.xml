<image>
	<profiles>
		<profile name="MATE_Compiz-Live" description="MATE-Compiz Spin Live DVD">
			<requires profile="MATE-Desktop"/>
			<requires profile="LiveInstall"/>
		</profile>
		<profile name="MATE_Compiz-Disk" description="MATE-Compiz Spin Install Disk">
			<requires profile="MATE-Desktop"/>
